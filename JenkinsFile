node {
    stages {
        stage('Clone') {
            steps {
                git "https://github.com/ahmedfou/helloworld.git"
            }
        }
        stage('Build') {
            steps {
                sh "javac Main.java"
            }
        }
        stage('Run') {
            steps {
                sh "java Main"
            }
        }
    }
}


